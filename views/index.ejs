<body>

  <div class="main">
    <div class="page1">
      <div class="left_placeholder"></div>
      <div class="left">
        <div class="logo" id="logoresp">
          <a href="/">
            <img
              src="https://res.cloudinary.com/dzotxhnz0/image/upload/v1686664022/ALL_PHOTO_OF_SPOTIFY/spologo_tcowjk.png"
              alt="">

          </a>
        </div>
        <div class="list listdiffer">
          <div class="item active">
            <a href="/spotify"><i class="ri-home-4-fill"></i>
              <h3>Home</h3>
            </a>
          </div>
          <div class="item">
            <a href="/search"><i class="ri-search-line"></i>
              <h3>Search</h3>
            </a>
          </div>

          <div class="item">
            <a href=""><i class="ri-contacts-book-upload-line"></i>
              <h3>Your Library</h3>
            </a>
          </div>
          <div class="item comingsoon">
            <a href=""><i class="ri-google-play-line"></i>
              <h3>Get App</h3>
            </a>
          </div>
        </div>
        <div class="list listdiffer2">
          <div class="item">
            <a href="/createplaylist"><i class="ri-add-box-fill"></i>
              <h3>Create Playlist</h3>
            </a>
          </div>
          <div class="item">
            <a href="/likedsongs"><i class="ri-heart-fill"></i>
              <h3>Liked Songs</h3>
            </a>
          </div>
        </div>
      </div>
      <div class="right">
        <div class="right_nav">
          <div class="right_lnav right_nav_comman">
            <div class="top_logo">
              <img src="../images/spologo.png" alt="">
            </div>
          </div>
          <div class="right_rnav right_nav_comman">
            <div class="right_auth">
              <div class="nav_sign">
                <a href="/sign">Sign up</a>
              </div>
              <div class="nav_log">
                <a href="/login">Log in</a>

              </div>
            </div>
          </div>
        </div>
        <div class="right_part">
          <div class="right_layer1">
            <div class="layer1_line">
              <a href="">
                <h2>Trending Songs</h2>
              </a>
              <a href="">Show all</a>
            </div>

            <div class="layer1_songs">
              <% song.forEach(function(dets){ %>

                <div class="song_card">
                  <a href="/playing/<%=dets._id%>">
                    <div class="song_card_temp">
                      <div class="playbtn"><i class="ri-play-fill"></i></div>
                      <div class="song_img">
                        <img src="<%=dets.image%>" alt="">
                      </div>
                      <div class="song_body">
                        <h6>
                          <%=dets.track %>
                        </h6>
                        <p>
                          <%=dets.artist%>
                        </p>
                      </div>
                    </div>
                  </a>
                </div>
                <% }) %>


            </div>
          </div>
          <div class="right_layer2">
            <div class="layer1_line">
              <a href="">
                <h2>Focus</h2>
              </a>
              <a href="">Show all</a>
            </div>

            <div class="layer1_songs clutter_in">
            </div>
          </div>
        </div>
      </div>
      <div class="top_mobile">
        <div class="top_logo">
          <img
            src="https://res.cloudinary.com/dzotxhnz0/image/upload/v1686664022/ALL_PHOTO_OF_SPOTIFY/spologo_tcowjk.png"
            alt="">

        </div>
        <div class="set">
          <i class="ri-settings-5-line"></i>
        </div>
      </div>
    </div>
    <div class="footer">
      <div class="foot_image">
    
      </div>
      <div class="foot_cont">
        <div class="foot_section">
          <div class="foot_one footcomman">
            <h2>Information</h2>
            <h3>Spotify Clone</h3>
